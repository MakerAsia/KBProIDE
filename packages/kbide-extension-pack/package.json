{
  "name": "kbide-extionsion-pack",
  "version": "1.0.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name kbide-extension-pack src/lib.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "material-design-icons-iconfont": "^5.0.1",
    "serial": "0.0.9",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.5.0",
    "vuetify": "^1.5.21"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/cli-service": "^4.0.5",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
