        "-I\"{platform}/sdk/include/openssl\"", "-I\"{platform}/sdk/include/protobuf-c\"", "-I\"{platform}/sdk/include/protocomm\"", 
        "-I\"{platform}/sdk/include/pthread\"", "-I\"{platform}/sdk/include/sdmmc\"", "-I\"{platform}/sdk/include/smartconfig_ack\"", 
